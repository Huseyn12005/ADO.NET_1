<Window x:Class="ADO.NET_1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ADO.NET_1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ComboBox Name="aut" ItemsSource="{Binding Authorrs}" SelectionChanged="aut_SelectionChanged" Width="250" Height="25" Margin="150,116,150,268"/>
        <ComboBox Name="cat"  ItemsSource="{Binding Categoryys}" IsEnabled="False"  Width="250" Height="25" Margin="150,280,150,104" />
    </Grid>
</Window>
